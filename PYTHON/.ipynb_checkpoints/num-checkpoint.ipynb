    "#find the inverse of a matrix using numpy\n",
    "import numpy as np\n",
    "mat = np.array([[1, 2], [3, 4]])\n",
    "inv_mat = np.linalg.inv(mat)\n",
    "print(\"Original Matrix:\")\n",
    "print(mat)\n",
    "print(\"Inverse Matrix:\")\n",
    "print(inv_mat)"
   ]
